{
  "name": "dorman-lakely-cartography",
  "version": "0.2.0",
  "description": "A FoundryVTT module for downloading and managing Dorman Lakely custom battlemaps with Patreon integration",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "prebuild": "node scripts/increment-build.js",
    "build": "vite build",
    "watch": "vite build --watch",
    "clean": "rm -rf dist",
    "build:db": "node utils/packs.mjs --pack",
    "build:json": "node utils/packs.mjs --unpack",
    "build:clean": "node utils/packs.mjs --clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci --maxWorkers=2",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"**/*.json\" \"**/*.md\" \"!packs/**\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"**/*.json\" \"**/*.md\" \"!packs/**\"",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major"
  },
  "keywords": [
    "foundry",
    "foundryvtt",
    "battlemap",
    "cartography",
    "patreon",
    "maps"
  ],
  "author": "Dorman Lakely",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/dorman-lakely-cartography.git"
  },
  "dependencies": {
    "classic-level": "^1.4.1",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.7.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "handlebars": "^4.7.8",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.1.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  },
  "private": true
}
