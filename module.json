{
  "id": "dorman-lakely-cartography",
  "title": "Dorman Lakely Cartography",
  "description": "A FoundryVTT module for downloading and managing Dorman Lakely custom battlemaps with Patreon integration.",
  "version": "1.0.1",
  "authors": [
    {
      "name": "Dorman Lakely",
      "url": "https://patreon.com/YOUR_PATREON_USERNAME"
    }
  ],
  "compatibility": {
    "minimum": "13",
    "verified": "13",
    "maximum": "13"
  },
  "relationships": {
    "requires": [
      {
        "id": "tagger",
        "type": "module"
      },
      {
        "id": "monks-active-tiles",
        "type": "module"
      },
      {
        "id": "enhanced-region-behavior",
        "type": "module",
        "reason": "Required for elevation region behaviors in imported maps"
      }
    ]
  },
  "esmodules": [
    "dist/main.js"
  ],
  "styles": [
    "styles/main.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "packs": [
    {
      "name": "maps",
      "label": "Dorman Lakely Maps",
      "path": "packs/maps",
      "type": "Scene",
      "system": [],
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    }
  ],
  "packFolders": [
    {
      "name": "Dorman Lakely Cartography",
      "sorting": "a",
      "packs": [
        "maps"
      ]
    }
  ],
  "url": "https://github.com/jesshmusic/dorman-lakely-cartography",
  "manifest": "https://raw.githubusercontent.com/jesshmusic/dorman-lakely-cartography/main/module.json",
  "download": "https://github.com/jesshmusic/dorman-lakely-cartography/releases/download/v1.0.1/module.zip",
  "license": "LICENSE",
  "readme": "README.md",
  "bugs": "https://github.com/jesshmusic/dorman-lakely-cartography/issues",
  "changelog": "https://github.com/jesshmusic/dorman-lakely-cartography/blob/main/CHANGELOG.md"
}
